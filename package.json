{
  "name": "askarthur",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.5.2",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "dev:web": "pnpm --filter @askarthur/web dev",
    "dev:mobile": "pnpm --filter @askarthur/mobile dev",
    "dev:telegram": "pnpm --filter @askarthur/web telegram:dev"
  },
  "devDependencies": {
    "turbo": "^2.8.10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
